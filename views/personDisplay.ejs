<div style="display: flex; justify-content: space-evenly;flex-wrap: wrap;">
    <% for(i=0;i< people.length;i++){ %>

        <div>
            <a class="movieLinks" href="/actors/<%= people[i].id %>">
                <div class="movieContainer" style="width: 205px;text-align: center;">
                    <img src=<%= people[i].url %>>
                    <div>

                        <%= people[i].name %>
                    </div>

                </div>
            </a>
        </div>
    <% } %>  

</div>